{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/main.ts"
    }
  ],
  "env": {
    "TS_NODE_COMPILER_OPTIONS": "{\"baseUrl\":\"./\",\"paths\":{\"@application/*\":[\"./src/application/*\"], \"@helpers/*\":[\"./src/helpers/*\"], \"@infra/*\":[\"./src/infra/*\"], \"@test/*\":[\"./src/test/*\"],}}"
  }
}